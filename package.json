{
    "name": "secure-store-redis",
    "version": "3.0.5",
    "description": "A simple wrapper to encrypt and decrypt data stored in Redis",
    "license": "MIT",
    "private": false,
    "packageManager": "pnpm@8.15.5",
    "engines": {
        "node": ">=16",
        "pnpm": ">=8"
    },
    "keywords": [
        "redis",
        "secure",
        "store",
        "encrypt",
        "decrypt",
        "client",
        "database"
    ],
    "author": {
        "name": "Nick Jennings",
        "email": "nick@silverbucket.net"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/",
        "src/"
    ],
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "publish": "pnpm build && pnpm publish",
        "test": "mocha -r ts-node/register src/*.test.ts",
        "lint": "prettier --check . && eslint --max-warnings 0 .",
        "lint:fix": "prettier --write .",
        "build": "tsc"
    },
    "dependencies": {
        "redis": "4.6.13"
    },
    "devDependencies": {
        "@types/chai": "4.3.14",
        "@types/debug": "4.1.12",
        "@types/eslint": "8.56.6",
        "@types/mocha": "10.0.6",
        "@types/node": "20.12.2",
        "@typescript-eslint/eslint-plugin": "7.4.0",
        "@typescript-eslint/parser": "7.4.0",
        "chai": "4.4.1",
        "debug": "4.3.4",
        "eslint": "8.57.0",
        "eslint-plugin-security-node": "1.1.4",
        "mocha": "10.4.0",
        "prettier": "3.2.5",
        "ts-node": "10.9.2",
        "typescript": "5.4.3"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/silverbucket/secure-store-redis.git"
    },
    "readmeFilename": "README.md",
    "bugs": {
        "url": "https://github.com/silverbucekt/secure-store-redis/issues"
    },
    "homepage": "https://github.com/silverbucket/secure-store-redis"
}
