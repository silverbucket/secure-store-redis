{
  "name": "secure-store-redis",
  "version": "2.0.5",
  "description": "A simple wrapper to encrypt and decrypt data stored in Redis",
  "license": "MIT",
  "private": false,
  "packageManager": "pnpm@8.0.0",
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "keywords": [
    "redis",
    "secure",
    "store",
    "encrypt",
    "decrypt",
    "client",
    "database"
  ],
  "author": {
    "name": "Nick Jennings",
    "email": "nick@silverbucket.net"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "test": "npm run build && jaribu",
    "lint": "prettier --check . && eslint --max-warnings 0 .",
    "lint:fix": "prettier --write . && eslint --max-warnings 0 --fix .",
    "build": "tsc"
  },
  "dependencies": {
    "redis": "4.6.6"
  },
  "devDependencies": {
    "@types/eslint": "8.4.6",
    "@types/node": "18.7.13",
    "@typescript-eslint/eslint-plugin": "5.36.0",
    "@typescript-eslint/parser": "5.36.0",
    "debug": "4.3.4",
    "eslint": "8.22.0",
    "eslint-plugin-security-node": "1.1.1",
    "jaribu": "2.2.3",
    "prettier": "2.8.8",
    "typescript": "4.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/silverbucket/secure-store-redis.git"
  },
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/silverbucekt/secure-store-redis/issues"
  },
  "homepage": "https://github.com/silverbucket/secure-store-redis"
}
